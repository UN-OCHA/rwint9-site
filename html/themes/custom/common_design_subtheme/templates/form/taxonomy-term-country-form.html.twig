<fieldset id="description">
  <legend>{{ 'Description'|t }}</legend>
  {{ element.name }}
  {{ element.field_shortname }}
  {{ element.field_longname }}
  {{ element.field_aliases }}
  {{ element.description }}
</fieldset>

<fieldset id="details" class="details">
  <legend>{{ 'Details'|t }}</legend>
  {{ element.field_iso3 }}
  {{ element.field_location }}
</fieldset>

<fieldset id="editorial-flags">
  <legend>{{ 'Editorial flags'|t }}</legend>
  {{ element.field_profile }}
  {{ element.field_timezone }}
</fieldset>

<fieldset id="actions">
  <legend class="accessibility">{{ 'Form submission and other actions'|t }}</legend>
  {{ element.actions }}
</fieldset>

{%
  set skip_elements = [
    'name',
    'field_shortname',
    'field_longname',
    'field_aliases',
    'description',
    'field_iso3',
    'field_location',
    'field_profile',
    'field_timezone',
    'actions',
  ]
%}

{% for key, child in element if key|first != '#' %}
  {{ key not in skip_elements ? child }}
{% endfor %}
