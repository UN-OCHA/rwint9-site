{#
/**
 * @file description_entity.html.twig
 * Default theme implementation to present Description entity data.
 *
 * This template is used when viewing Description entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_description_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('description_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
